  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/studiohyperdrive/api-docs/issues"
  },
  "homepage": "https://github.com/studiohyperdrive/api-docs#readme",
  "dependencies": {
    "express": "4.16.3",
    "glob": "7.1.2",
    "pretty-swag": "0.1.144",
    "ramda": "0.25.0",
    "swagger-jsdoc": "1.9.7",
    "swagger-parser": "5.0.0"
  },
  "devDependencies": {
    "@studiohyperdrive/eslint-config": "3.0.2",
